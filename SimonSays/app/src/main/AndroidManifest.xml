<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.simonsay">
    <uses-permission android:name="android.permission.VIBRATE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SimonGame"/>
        <activity android:name=".SimonGameActivity" />
        <activity android:name=".TableScoreActivity" />
        <activity android:name=".active"/>
        <activity android:name=".activ2" />
        <activity android:name=".active3" />
        <activity android:name=".activ4" />
        <activity android:name=".activ5" />
        <activity android:name=".LevelsActivity" />
    </application>

</manifest>